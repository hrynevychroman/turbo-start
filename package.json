{
  "name": "turbo-template",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "check-types": "turbo check-types",
    "clean": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
    "commitlint": "commitlint --edit",
    "build:do": "pnpm install --production=false && turbo build && rm -rf node_modules && pnpm install --production --frozen-lockfile",
    "prepare": "simple-git-hooks"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.4",
    "@internationalized/number": "^3.5.3",
    "@tanstack/vue-query": "^5.51.4",
    "@tanstack/vue-table": "^8.19.3",
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^10.11.0",
    "@vueuse/integrations": "^10.11.0",
    "@vueuse/router": "^10.11.0",
    "fuse.js": "^7",
    "i18next": "^23.12.1",
    "libphonenumber-js": "^1.11.4",
    "nanoid": "^5.0.7",
    "ofetch": "^1.3.4",
    "pathe": "^1.1.2",
    "radash": "^12.1.0",
    "radix-vue": "^1.9.1",
    "vee-validate": "^4.13.2",
    "vue-sonner": "1.1.2",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/format": "^19.3.0",
    "@types/eslint": "^8.56.10",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.14.11",
    "@vitejs/plugin-vue": "^5.0.5",
    "defu": "^6.1.4",
    "lint-staged": "^15.2.7",
    "simple-git-hooks": "^2.11.1",
    "turbo": "^2.0.7",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vite-plugin-vue-layouts": "^0.11.0"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm commitlint $1",
    "pre-commit": "turbo check-types"
  }
}
